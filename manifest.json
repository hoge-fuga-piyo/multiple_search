{
  "name": "Multiple Word Searcher",
  "description": "Multiple Word Searcher",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Multiple Word Searcher"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*",
        "file://*/*"
      ],
      "js": [
        "src/content.js"
      ],
      "css": [
        "src/content.css"
      ]
    }
  ]
}